doctype html
html
	head
		title BootStrap 3 Column Skeleton
		meta(charset="utf-8")
		meta(name="viewport", content="width=device-width, inital-scale=1")
		link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css")
		link(rel="stylesheet", href="styles.css")
		
	body
		.container
			.row
				.col-md-4
					h1 SiteTitle
					
				ul.col-md-8.list-inline.text-right(style="margin-top:20px")
						li
							a(href=""): .glyphicon.glyphicon-envelope Newsletter
						li
							a(href=""): .glyphicon.glyphicon-leaf RSS
						li 
							form.form-inline(role="form")
								.form-group
									lable.sr-only Email Address
									input.form-control(type="email", placeholder="Enter Email")
									button.btn.btn-default(type="submit") Sign Up!
									
			nav.row
				.col-sm-12
				ul.nav.nav-tabs(style="background: #eee; padding: 20px 10px 0 0; border-radius: 5px 5px 0 0")
					li: a(href="") Tab 1
					li: a(href="") Tab 2
					li: a(href="") Tab 3
					li: a(href="") Tab 4
					li: a(href="") Tab 5
					li: a(href="") Tab 6
					
			.row(style="padding-top: 10px")
			
				.col-sm-5
					include col-main					
				.col-sm-4.col-secondary
					include col-secondary
				.col-sm-3
					include col-tertiary